'.text.xml':
  'D3R Field SelectCallback':
    'prefix': 'select'
    'body': '<field>\n    <name>$1</name>\n    <!-- <dbname>${1/[ ]/_/ig}</dbname> -->\n    <type>SelectCallback</type>\n    <callback>$2</callback>\n    <!-- <default>$3</default> -->\n    <required>False</required>\n    <listing>False</listing>\n    <hasblank>True</hasblank>\n</field>'